\begin_layout Enumerate
הטענה נכונה:
\begin_inset Formula 
\begin{gather*}
\begin{array}{c}
f_{1}=\Theta\left(g_{1}\right)\\
f_{2}=\Theta\left(g_{2}\right)
\end{array}\implies\begin{array}{c}
\exists c_{1}>0,n_{1};\;\forall n\geq n_{1}:\;f_{1}\leq cg_{1}\\
\exists c_{2}>0,n_{2};\;\forall n\geq n_{2}:\;f_{2}\leq cg_{2}
\end{array}\;\land\;\begin{array}{c}
\exists c'_{1}>0,n'_{1};\;\forall n\geq n'_{1}:\;f_{1}\geq c_{1}'g_{1}\\
\exists c'_{2}>0,n'_{2};\;\forall n\geq n'_{2}:\;f_{2}\geq c_{2}'g_{2}
\end{array}\\
\frac{f_{1}}{f_{2}}\leq\frac{c_{1}g_{1}}{c_{2}'g_{2}}\implies\exists c=\frac{c_{1}}{c_{2}'},n_{0}=\max\left\{ n_{1},n'_{2}\right\} :\quad\frac{f_{1}}{f_{2}}\leq c\frac{g_{1}}{g_{2}}\implies\frac{f_{1}}{f_{2}}=O\left(\frac{g_{1}}{g_{2}}\right)\\
\frac{f_{1}}{f_{2}}\geq\frac{c_{1}'g_{1}}{c_{2}g_{2}}\implies\exists c=\frac{c'_{1}}{c_{2}},n_{0}=\max\left\{ n'_{1},n_{2}\right\} :\quad\frac{f_{1}}{f_{2}}\geq c\frac{g_{1}}{g_{2}}\implies\frac{f_{1}}{f_{2}}=\Omega\left(\frac{g_{1}}{g_{2}}\right)\\
\Downarrow\\
\frac{f_{1}}{f_{2}}=\Theta\left(\frac{g_{1}}{g_{2}}\right)\;_{\blacksquare}
\end{gather*}

\end_inset


\end_layout

\begin_layout Enumerate
הטענה נכונה:
\begin_inset Formula 
\begin{gather*}
\left|\log\frac{f_{1}}{g_{1}}\right|=O\left(f_{2}\right)\implies\exists c_{1}>0,n_{1};\;\forall n\geq n_{1}:\;\left|\log\frac{f_{1}}{g_{1}}\right|\leq c_{1}f_{2}\\
